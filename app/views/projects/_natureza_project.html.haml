.label-upme
  - if @project.user.empresa?
    = 'Natureza'
  - else
    = 'O projeto atende a organizações'

.row
  - if @project.nat_publica
    .col.s12.m4.center-align
      .btn-upme-outline-grey 🏢  Empresa privada
      %br
  - if @project.nat_privada
    .col.s12.m4.center-align
      .btn-upme-outline-grey 🏛️ Empresa pública
      %br
      
  - if @project.nat_ong
    .col.s12.m4.center-align
      .btn-upme-outline-grey 🙌  ONG/Social
      %br
      
